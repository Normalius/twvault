{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console", "Serilog.Sinks.Debug", "Serilog.Sinks.Async" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "MinimumLevel": {
              "Default": "Information"
            },
            "WriteTo": [
              { "Name": "Console" },
              { "Name": "Debug" },
              {
                "Name": "Async",
                "Args": {
                  "configure": [
                    {
                      "Name": "File",
                      "Args": {
                        "pathFormat": "../Logs/log-{Date}.log",
                        "buffered": false
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "MinimumLevel": {
              "Default": "Information"
            },
            "WriteTo": [
              {
                "Name": "Async",
                "Args": {
                  "configure": [
                    {
                      "Name": "File",
                      "Args": {
                        "pathFormat": "../DevLogs/log-{Date}-ip.log",
                        "buffered": false
                      }
                    }
                  ]
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "StartsWith(@Properties['SourceContext'], 'TW.Vault.Lib.Security.IPLoggingInterceptionAttribute')"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Vault": "Server=localhost; Database=vault; User Id=twu_vault; Password=password; Port=54321"
  },
  "PageSize": 50,
  "Security": {
    "MinimumRequiredPriveleges": 1,
    "RestrictAccessWithinTribe": false,
    "EnableScriptFilter": false,
    "RestrictSitterAccess": true,
    "UseEncryption": true,
    "PublicScripts": [
      "main.min.js"
    ],

    "ForceEnableObfuscatedScripts": true

    //"ForcedKey": "0356f56d-f984-4224-8d18-25108ce4b6ae",
    //"ForcedPlayerId": 11473626,
    //"ForcedTribeId": 776
  },
  "Initialization": {
    "ServerHostname": "localhost",
    "ServerBasePath": "/"
  },
  "Rankings": {
    "EnableRankingsService": true,
    "RefreshCheckIntervalSeconds": 300
  },
  "Behavior": {
    "DisableFakeScript": true
  }
}
