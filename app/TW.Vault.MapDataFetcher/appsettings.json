{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console", "Serilog.Sinks.Debug", "Serilog.Sinks.Async" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "pathFormat": "../Logs/log-{Date}.log",
                "buffered": false
              }
            }
          ]
        }
      }
    ]
  },
  "CachingFilePath": "cache",
  "DataBatchSize": 500,
  "CheckDelaySeconds": 10
}
